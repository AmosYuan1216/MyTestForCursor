{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
  "redirects": [
    {
      "source": "/",
      "destination": "/publications.html",
      "permanent": true
    }
  ]
}
